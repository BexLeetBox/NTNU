    "def calculate_dx (a, b, n):\n",
    "\treturn (b-a)/float(n)\n",
    "\n",
    "def rect_rule (f, a, b, n):\n",
    "\ttotal = 0.0\n",
    "\tdx = calculate_dx(a, b, n)\n",
    "\tfor k in range (0, n):\n",
    "        \ttotal = total + f((a + (k*dx)))\n",
    "\treturn dx*total\n",
    "\n",
    "def f(x):\n",
    "    return 1/(1+x**6)\n",
    "\n",
    "print(rect_rule(f, 0, 2, 10))\n"
